						"          country_code_2_digit,",
						"          country_code_3_digit,",
						"          population,",
						"          reported_date,",
						"          source,",
						"          transformed_at),",
						"     pivotBy(indicator, ['Daily ICU occupancy', 'Daily hospital occupancy']),",
						"     count = first(toInteger(value), true()),",
						"     columnNaming: '$V_$N',",
						"     lateral: true) ~> PivotDailyIndicatior",
