{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid19-df-udemy-course"
		},
		"Covid19 Azure Blob Storage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Covid19 Azure Blob Storage'"
		},
		"Covid19 Azure Data Lake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Covid19 Azure Data Lake'"
		},
		"Covid19SqlsrvUdemyCourse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Covid19SqlsrvUdemyCourse'"
		},
		"Covid19 Azure Data Lake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covid19dludemycourse.dfs.core.windows.net/"
		},
		"ECDC MONK SITE HTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{concat(linkedService().SCHEME, ':\\\\', linkedService().HOST)}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid19-df-udemy-course_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Covid19 Azure Blob Storage_connectionString": {
						"value": "[parameters('Covid19 Azure Blob Storage_connectionString')]"
					},
					"Covid19 Azure Data Lake_accountKey": {
						"value": "[parameters('Covid19 Azure Data Lake_accountKey')]"
					},
					"Covid19SqlsrvUdemyCourse_connectionString": {
						"value": "[parameters('Covid19SqlsrvUdemyCourse_connectionString')]"
					},
					"Covid19 Azure Data Lake_properties_typeProperties_url": {
						"value": "[parameters('Covid19 Azure Data Lake_properties_typeProperties_url')]"
					},
					"ECDC MONK SITE HTTP_properties_typeProperties_url": {
						"value": "[parameters('ECDC MONK SITE HTTP_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covid19-df-udemy-course_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covid19-df-udemy-course_ArmTemplate_0')]"
			]
		}
	]
}