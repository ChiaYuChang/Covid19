{
	"name": "ECDC MONK SITE HTTP",
	"properties": {
		"parameters": {
			"HOST": {
				"type": "string",
				"defaultValue": "raw.githubusercontent.com"
			},
			"SCHEME": {
				"type": "string",
				"defaultValue": "https"
			}
		},
		"annotations": [],
		"type": "HttpServer",
		"typeProperties": {
			"url": "@{concat(linkedService().SCHEME, ':\\\\', linkedService().HOST)}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		}
	}
}